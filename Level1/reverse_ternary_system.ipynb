   "source": [
    "## 3진법 뒤집기\n",
    "https://school.programmers.co.kr/learn/courses/30/lessons/68935"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "64113eb6",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    answer = 0\n",
    "    str_3 = []\n",
    "    \n",
    "    while n>0:\n",
    "        str_3.append(n%3)\n",
    "        n = n//3\n",
    "\n",
    "    for i, v in enumerate(str_3):\n",
    "        answer += v*(3**(len(str_3)-1-i))\n",
    "    \n",
    "    return answer"
   ]
