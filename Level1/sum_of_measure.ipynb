   "source": [
    "## 약수의 합\n",
    "https://school.programmers.co.kr/learn/courses/30/lessons/12928"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "cfaa476a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    answer = 0\n",
    "    for i in range(1, n+1):\n",
    "        if n%i==0:\n",
    "            answer+=i\n",
    "    \n",
    "    return answer"
   ]
