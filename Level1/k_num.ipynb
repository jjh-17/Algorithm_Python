   "outputs": [],
   "source": [
    "def solution(array, commands):\n",
    "    answer = []\n",
    "    \n",
    "    for com in commands:\n",
    "        answer.append((sorted(array[com[0]-1:com[1]]))[com[2]-1])\n",
    "        \n",
    "    \n",
    "    return answer"
   ]
