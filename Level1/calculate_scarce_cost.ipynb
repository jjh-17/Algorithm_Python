   "id": "5d8a530f",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(price, money, count):\n",
    "    answer = -1\n",
    "    cost = 0\n",
    "    \n",
    "    for i in range(1, count+1):\n",
    "        cost += (price*i)\n",
    "    answer = max(0, cost - money)\n",
    "    return answer"
   ]
