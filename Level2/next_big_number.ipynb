   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    count1 = bin(n).count('1')\n",
    "    \n",
    "    while True:\n",
    "        n = n+1\n",
    "        if bin(n).count('1') == count1:\n",
    "            break\n",
    "    \n",
    "    \n",
    "    return n"
   ]
