   "source": [
    "## h-index\n",
    "https://school.programmers.co.kr/learn/courses/30/lessons/42747"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "57e0ddd8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(citations):\n",
    "    citations.sort(reverse = True)\n",
    "    answer= len(citations)\n",
    "\n",
    "    for i, h in enumerate(citations):\n",
    "        if i >= h:\n",
    "            answer = i\n",
    "            break\n",
    "    \n",
    "    return answer"
   ]
