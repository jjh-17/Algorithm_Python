   "source": [
    "## 점프와 순간이동\n",
    "https://school.programmers.co.kr/learn/courses/30/lessons/12980"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "e6364e52",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    ans = 0\n",
    "    \n",
    "    while n:\n",
    "        if n%2 == 1:\n",
    "            n-=1\n",
    "            ans+=1\n",
    "        else:\n",
    "            n = n//2\n",
    "\n",
    "    return ans"
   ]
  },
